{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src/",
    "paths": {
      "@components/*": [
        "components/*",
        "components/icons/*",
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    },
    "jsx": "preserve"
  }
}